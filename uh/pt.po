#: horizons/i18n/guitranslations.py:51
msgid "Pavilion: Fulfills religious needs of sailors."
msgstr "Pavilhão: Atende as necessidades religiosas dos marinheiros."

#: horizons/i18n/guitranslations.py:52
msgid "Fisherman: Fishes the sea, produces food."
msgstr "Pescador: Pesca no mar, produz comida."

#: horizons/i18n/guitranslations.py:53
msgid "Hunter: Hunts wild forest animals, produces food."
msgstr "Caçador: Caça animais selvagens na floresta, produz comida."

#: horizons/i18n/guitranslations.py:54
msgid "Signal fire: Allows the player to trade with the free trader."
msgstr "Sinal de fogo: Permite ao jogador trocar com o mercador livre."

#: horizons/i18n/guitranslations.py:55
msgid "Lumberjack: Chops down trees and turns them into boards."
msgstr "Lenhador: Corta árvores e converte-as em tábuas."

#: horizons/i18n/guitranslations.py:56
msgid "Main square: Supplies citizens with goods."
msgstr "Praça principal: Fornece mercadorias aos cidadãos."

#: horizons/i18n/guitranslations.py:57
msgid "Tent: Houses your inhabitants."
msgstr "Tenda: Aloja os seus habitantes."

#: horizons/i18n/guitranslations.py:58
msgid "Storage: Extends stock and provides collectors."
msgstr ""
"Armazenamento: Extende a capacidade de armazenamento e fornece "
"transportadores."

#: horizons/i18n/guitranslations.py:59
msgid "Trail: Needed for collecting goods."
msgstr "Caminho: Necessário para transportar mercadorias."

#: horizons/i18n/guitranslations.py:60 sqlite/Building
msgid "Tree"
msgstr "Árvore"

#: horizons/i18n/guitranslations.py:64 horizons/i18n/guitranslations.py:83
msgid "Fields"
msgstr "Plantações"

#: horizons/i18n/guitranslations.py:65
msgid "Pioneer buildings"
msgstr "Edifícios de pioneiros"

#: horizons/i18n/guitranslations.py:66
msgid "Military"
msgstr "Militar"

#: horizons/i18n/guitranslations.py:68
msgid "Boat builder: Builds boats and small ships. Built on coast."
msgstr ""
"Construtor de barcos: Contrói barcos e navios pequenos. Construido na costa."

#: horizons/i18n/guitranslations.py:69
msgid "Brickyard: Turns clay into bricks."
msgstr "Fábrica de tijolo: Converte barro em tijolos."

#: horizons/i18n/guitranslations.py:70
msgid "Clay pit: Gets clay from deposit."
msgstr "Poço de barro: Extrai barro do depósito."

#: horizons/i18n/guitranslations.py:71
msgid "Distillery: Turns sugar into liquor."
msgstr "Destilaria: Converte o açucar em álcool."

#: horizons/i18n/guitranslations.py:72
msgid "Farm: Grows field crops and raises livestock."
msgstr "Quinta: Produz campos de cultivo e cria gado."

#: horizons/i18n/guitranslations.py:73
msgid "Pasture: Raises sheep. Produces wool. Needs a farm."
msgstr "Pasto: Cria ovelhas. Produz lã. Requer uma quinta."

#: horizons/i18n/guitranslations.py:74
msgid "Potato field: Yields food. Needs a farm."
msgstr "Plantação de batatas: Produz comida. Requer uma quinta."

#: horizons/i18n/guitranslations.py:75
msgid "Sugarcane field: Used in liquor production. Needs a farm."
msgstr ""
"Plantação de cana de açúcar: Utilizado na produção de alcoól. Requer uma "
"quinta."

#: horizons/i18n/guitranslations.py:76
msgid "Wooden tower: Defends your settlement."
msgstr "Torre de madeira: Defende a povoação."

#: horizons/i18n/guitranslations.py:77
msgid "Village school: Provides education."
msgstr "Escola: Fornece educação."

#: horizons/i18n/guitranslations.py:78
msgid "Weaver: Turns lamb wool into cloth."
msgstr "Tecelão: Converte a lã em tecido."

#: horizons/i18n/guitranslations.py:82
msgid "Mining"
msgstr "Exploração de minas"

#: horizons/i18n/guitranslations.py:84
msgid "Settler buildings"
msgstr "Edifícios de colonos"

#: horizons/i18n/guitranslations.py:86
msgid "Butchery: Needs pigs or cattle. Produces food."
msgstr "Matadouro: Precisa de porcos ou gado vacum. Produz comida."

#: horizons/i18n/guitranslations.py:87
msgid "Cattle run: Raises cattle. Needs a farm."
msgstr "Pasto: Cria gado. Precisa de uma quinta."

# 76%
#: horizons/i18n/guitranslations.py:88
msgid "Charcoal burning: Burns a lot of boards to charcoal."
msgstr "Carvoaria: Queima molhos de tábuas para fazer carvão."

#: horizons/i18n/guitranslations.py:89
msgid "Mine: Gets ores from deposit."
msgstr "Mina: Extrai minério dos depósitos."

#: horizons/i18n/guitranslations.py:90
msgid "Pigsty: Raises pigs. Needs a farm."
msgstr "Pocilga: Cria porcos. Precisa de uma quinta."

#: horizons/i18n/guitranslations.py:91
msgid "Salt ponds: Evaporates salt. Built on sea coast."
msgstr "Salinas: Água evapora para produzir sal. Construído na costa marítima."

#: horizons/i18n/guitranslations.py:92
msgid "Smeltery: Refines all kind of ores."
msgstr "Fundição: Refina todos os tipos de minério."

# A tavern is also a place where wine is sold, shouldn't it be mentioned?
#: horizons/i18n/guitranslations.py:93
msgid "Tavern: Provides get-together."
msgstr "Taberna: Oferece um local para reuniões populares."

